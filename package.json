{
  "name": "nevermiss",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "clear": "expo start clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --profile development --platform android",
    "build:ios": "eas build --profile development-simulator --platform ios",
    "build:all": "eas build -p all",
    "preview:android": "eas build --profile preview --platform android",
    "preview:ios": "eas build --profile preview --platform ios",
    "publish": "eas build --profile production --platform all",
    "postpublish": "eas submit --platform all",
    "prevPublish": "expo publish --release-channel preview"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^0.17.2",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "expo": "~47.0.8",
    "expo-av": "~13.0.3",
    "expo-camera": "~13.1.0",
    "expo-dev-client": "~2.0.1",
    "expo-image-manipulator": "~11.0.0",
    "expo-image-picker": "~14.0.2",
    "expo-location": "~15.0.1",
    "expo-permissions": "~14.0.0",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-web": "~0.18.9",
    "react-native-webview": "11.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "npm-run-all": "^4.1.5"
  },
  "private": true
}
